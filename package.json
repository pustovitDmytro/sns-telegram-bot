    "dotenv": "10.0.0",
    "express": "4.17.1",
    "handlebars": "4.7.7",
    "livr": "2.3.3",
    "livr-extra-rules": "1.2.1",
    "logger-decorator": "1.6.0",
    "ms": "2.1.3",
    "myrmidon": "1.6.1",
    "qs": "6.10.1",
    "winston": "3.3.3",
    "yargs": "17.2.1"
  }
}
